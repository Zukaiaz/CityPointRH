@model IEnumerable<CityPoint_RoomHire.Models.VenueEquipment>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IsIncluded)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Venue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Equipment)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IsIncluded)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Venue.VenuesId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Equipment.EquipmentHireId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VenueEquipmentId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VenueEquipmentId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VenueEquipmentId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
